<%= form_for @recipe do |f| %>
  Title: <%= f.text_field :title %>
  <br></br>
  <%= f.fields_for :ingredients do |ing| %>
  <li>
  <%= ing.label :name %>
  <%= ing.text_field :name %>
  <%= ing.label :quantity %>
  <%= ing.text_field :quantity %>
  </li>
  <%end%>
  <br></br>
  <%= f.submit %>
<% end %>
